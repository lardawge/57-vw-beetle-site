<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Use the title from a page's frontmatter if it has one -->
    <%= auto_display_meta_tags %>
    <!-- Favicons -->
    <link rel="icon" type="image/x-icon" href="<%= image_path 'favicon.ico' %>">
    <link rel="icon" type="image/png" sizes="16x16" href="<%= image_path 'favicon-16x16.png' %>">
    <link rel="icon" type="image/png" sizes="32x32" href="<%= image_path 'favicon-32x32.png' %>">
    <link rel="apple-touch-icon" href="<%= image_path 'apple-touch-icon.png' %>">
    <link rel="manifest" href="<%= image_path 'site.webmanifest' %>">
    <meta name="theme-color" content="#ffffff">
    <%= stylesheet_link_tag "style" %>
  </head>
  <body>
    <%= yield %>
    <%= javascript_include_tag "site.min" %>
  </body>
</html>
